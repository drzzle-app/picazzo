    "util": "^0.10.3",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:less && npm run lint:js"
    }
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "files": [
    "dist/fonts/**/*",
    "dist/css/*",
    "dist/js/**/*",
    "dist/icons/**/*"
  ],
  "browserslist": [
    "> 1%",
    "last 5 versions",
    "not ie <= 8"
  ]
}
