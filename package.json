    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lesshint": "^6.3.7",
    "lesshint-reporter-stylisher": "^1.0.0",
    "mocha": "^6.0.2",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
